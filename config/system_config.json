{
    "protocol": {
      "version": "1.0",
      "message_max_size_bytes": 10485760,
      "nonce_size_bytes": 16
    },
    "crypto": {
      "aes_mode": "GCM",
      "aes_key_size": 256,
      "kyber_variant": 1024,
      "dilithium_variant": 5,
      "hash_algorithm": "SHA3-512"
    },
    "security": {
      "min_password_length": 12,
      "force_totp": true,
      "session_key_lifetime_mins": 60,
      "max_file_size_mb": 10
    },
    "blockchain": {
      "transaction_confirmation_blocks": 1,
      "allow_file_overwrite": false
    }
  }